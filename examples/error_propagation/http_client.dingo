package main

import "net/http"
import "io"
import "fmt"

// fetchURL demonstrates error propagation with http.Get and io.ReadAll
func fetchURL(url: string) (string, error) {
	let resp = http.Get(url)?
	defer resp.Body.Close()

	let body = io.ReadAll(resp.Body)?
	return string(body), nil
}

func main() {
	let content = fetchURL("https://example.com")?
	fmt.Println(content)
}
