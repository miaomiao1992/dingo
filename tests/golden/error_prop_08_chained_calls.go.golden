package main

import "fmt"

type Config struct {
	Host string
	Port int
}

func pipeline(path string) (*Config, error) {
	data, err := ReadFile(path)
	// dingo:s:1
	if err != nil {
		return nil, fmt.Errorf("failed to read config: %w", err)
	}
	// dingo:e:1
	var cfg Config
	err = Unmarshal(data, &cfg)
	// dingo:s:1
	if err != nil {
		return nil, fmt.Errorf("failed to parse config: %w", err)
	}
	// dingo:e:1
	return &cfg, nil
}
