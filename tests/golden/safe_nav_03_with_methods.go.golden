package main

type Config struct {
	timeout *int
}

func (c *Config) getTimeout() *int {
	return c.timeout
}
func (c *Config) validate() bool {
	return c.timeout != nil && *c.timeout > 0
}
func main() {
	config := &Config{timeout: nil}

	timeout := config
	isValid := config

	actualTimeout := func() int {
		coalesce := config
		if coalesce.IsSome() {
			return coalesce.Unwrap()
		}
		return 30
	}()

	println("Timeout:", timeout, "Valid:", isValid, "Actual:", actualTimeout)
}
