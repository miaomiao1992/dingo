package main

import "errors"

enum Result {
	Ok(float64),
	Err(error),
}

func divide(a: float64, b: float64) Result {
	if b == 0.0 {
		return ResultErr(errors.New("division by zero"))
	}
	return ResultOk(a / b)
}

func main() {
	let result = divide(10.0, 2.0)
	if result.IsOk() {
		let v = *result.ok
	}
}
