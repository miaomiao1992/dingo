package main

import (
	"errors"
)

type ResultTag uint8

const (
	ResultTagOk ResultTag = iota
	ResultTagErr
)

type Result struct {
	tag   ResultTag
	ok  *int
	err *error
}

func Result_Ok(arg0 int) Result {
	return Result{tag: ResultTagOk, ok: &arg0}
}
func Result_Err(arg0 error) Result {
	return Result{tag: ResultTagErr, err: &arg0}
}
func (e Result) IsOk() bool {
	return e.tag == ResultTagOk
}
func (e Result) IsErr() bool {
	return e.tag == ResultTagErr
}
func analyzeResults(r1 Result, r2 Result, r3 Result) string {

	elem0, elem1, elem2 := r1, r2, r3

	switch elem0.tag {
	case ResultTagErr:
		switch elem1.tag {
		default:
			switch elem2.tag {
			default:

				e := *elem0.err
				return "First failed immediately: " + e.Error()
			}
		}
	case ResultTagOk:
		switch elem1.tag {
		case ResultTagOk:
			switch elem2.tag {
			case ResultTagErr:

				_ := *elem0.ok
				_ := *elem1.ok
				e := *elem2.err
				return "First two ok, third failed: " + e.Error()
			case ResultTagOk:

				x := *elem0.ok
				y := *elem1.ok
				z := *elem2.ok
				return "All succeeded: sum = " + string(x+y+z)
			}
		}
	default:
		switch elem1.tag {
		case ResultTagErr:
			switch elem2.tag {
			default:

				e := *elem1.err
				return "Second failed: " + e.Error()
			}
		default:
			switch elem2.tag {
			case ResultTagErr:

				e := *elem2.err
				return "Third failed: " + e.Error()
			}
		}
	}
	panic("unreachable: match is exhaustive")

}
func main() {
	r1 := Result_Ok(10)
	r2 := Result_Err(errors.New("database error"))
	r3 := Result_Ok(30)
	result := analyzeResults(r1, r2, r3)
	println(result)
}
