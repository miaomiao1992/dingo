	if err != nil {
		return nil, err
	}
	// dingo:e:1
	var data = tmp
	var result map[string]interface{}
	tmp1, err1 := json.Unmarshal(data, &result)

	if err1 != nil {
		return nil, err1
	}
	// dingo:e:1
	var err = tmp1
	return result, nil
}
