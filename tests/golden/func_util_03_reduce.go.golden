package main

func reduce(arr []int, initial int, f func(int, int) int) int {
	result := initial
	for _, v := range arr {
		result = f(result, v)
	}
	return result
}
func main() {
	numbers := []int{1, 2, 3, 4, 5}

	sum := reduce(numbers, 0, func(acc __TYPE_INFERENCE_NEEDED, x __TYPE_INFERENCE_NEEDED) { return acc + x })

	product := reduce(numbers, 1, func(acc __TYPE_INFERENCE_NEEDED, x __TYPE_INFERENCE_NEEDED) { return acc * x })

	max := reduce(numbers, numbers[0], func(acc __TYPE_INFERENCE_NEEDED, x __TYPE_INFERENCE_NEEDED) {
		if x > acc {
			return x
		}
		return acc
	})

	println("Sum:", sum, "Product:", product, "Max:", max)
}
