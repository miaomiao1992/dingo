// Example 1: Pattern match on Result[T,E]
func processResult(result Result[int, error]) int {

	scrutinee := result
	switch scrutinee.tag {
	case ResultTagOk:
		value := *scrutinee.ok

		value * 2
	case ResultTagErr:
		e := scrutinee.err

		0
	}
	panic("unreachable: match is exhaustive")

}
// Example 2: Pattern match on Option[T]
func processOption(opt Option[string]) string {

	scrutinee2 := opt
	switch scrutinee2.tag {
	case OptionTagSome:
		s := *scrutinee2.some

		s
	case OptionTagNone:

		"default"
	}
	panic("unreachable: match is exhaustive")

}
// Example 3: Pattern match with wildcard
func handleStatus(status Status) string {

	scrutinee3 := status
	switch scrutinee3.tag {
	case ActiveTag:

		"running"
	case PendingTag:

		"waiting"
	default:

		"unknown"
	}
	panic("unreachable: match is exhaustive")

}
// Example 4: Pattern match in assignment context
func doubleIfPresent(opt Option[int]) Option[int] {
	var result OptionInt

	scrutinee4 := opt
	switch scrutinee4.tag {
	case OptionTagSome:
		x := *scrutinee4.some

		result = Some(x * 2)
	case OptionTagNone:

		result = Option_int_None()
	}
	panic("unreachable: match is exhaustive")
	return result

	return result
}
// Example 5: Nested pattern matching
func processNested(result Result[Option[int], error]) int {

	scrutinee5 := result
	switch scrutinee5.tag {
	case ResultTagOk:
		inner := *scrutinee5.ok

		{

			scrutinee := inner
			switch scrutinee.tag {
			case OptionTagSome:
				val := *scrutinee.some

				val
			}
			panic("unreachable: match is exhaustive")

		}
	case ResultTagErr:
		e := scrutinee5.err

		-1
	}
	panic("unreachable: match is exhaustive")

}
