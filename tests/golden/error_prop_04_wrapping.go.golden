package main

import (
	"fmt"
	"os"
)

func readUserConfig(username string) ([]byte, error) {
	path := "/home/" + username + "/config.json"
	tmp1, err1 := os.ReadFile(path)

	if err1 != nil {
		return nil, fmt.Errorf("failed to read user config: %w", err1)
	}
	// dingo:e:1
	var data = tmp1
	return data, nil
}
