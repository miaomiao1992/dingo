package main

// Test: Nested ternary operators
// Feature: Ternary operator nesting
// Complexity: intermediate

func main() {
	let age = 25
	let hasLicense = true

	// Nested ternary
	let canDrive = age >= 18 ? (hasLicense ? "yes" : "no license") : "too young"

	// Ternary in assignment
	let score = 85
	let grade = score >= 90 ? "A" : (score >= 80 ? "B" : (score >= 70 ? "C" : "F"))

	println("Can drive:", canDrive)
	println("Grade:", grade)
}
