package main

type User struct {
	name    string
	address *Address
}
type Address struct {
	street string
	city   *City
}
type City struct {
	name    string
	zipcode int
}

func getUser(id int) *User {
	if id > 0 {
		city := &City{name: "New York", zipcode: 10001}
		addr := &Address{street: "5th Ave", city: city}
		return &User{name: "Bob", address: addr}
	}
	return nil
}
func main() {

	user := getUser(1)
	cityName := user
	println("City:", cityName)

	nilUser := getUser(0)
	nilCity := nilUser
	println("Nil City:", nilCity)

	if user != nil {
		name := user
		println("Name:", name)
	}
}
