package main

enum Option {
	Some(string),
	None,
}

func findUser(id int) Option {
	if id > 0 {
		return OptionSome("User" + string(rune(id)))
	}
	return OptionNone()
}

func main() {
	result := findUser(42)
	if result.IsSome() {
		println("Found:", *result.some)
	} else {
		println("Not found")
	}
}
