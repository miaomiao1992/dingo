type Config struct {
	host string
	port int
}
func main() {

	name := StringOptionNone()
	display := func() __INFER__ {
		coalesce1 := name
		if coalesce1.IsSome() {
			return coalesce1.Unwrap()
		}
		return getDefaultName()
	}()

	port := IntOptionNone()
	finalPort := (func() __INFER__ {
		if port.IsSome() {
			return __UNWRAP__(port)
		}
		return 8080
	}()) + 1000

	config := getConfig()
	hostOpt := func() __INFER__ {
		if config.IsNone() {
			return __INFER___None()
		}
		config1 := config.Unwrap()
		return config1.host
	}()
	host := func() __INFER__ {
		coalesce2 := hostOpt
		if __IS_SOME__(coalesce2) {
			return __UNWRAP__(coalesce2)
		}
		coalesce3 := getEnv("HOST")
		if __IS_SOME__(coalesce3) {
			return __UNWRAP__(coalesce3)
		}
		return "localhost"
	}()

	user := findUser(42)
	userUpper := func() __INFER__ {
		if user.IsNone() {
			return __INFER___None()
		}
		user2 := user.Unwrap()
		return user2.toUpper()
	}()
	username := func() __INFER__ {
		coalesce4 := userUpper
		if __IS_SOME__(coalesce4) {
			return __UNWRAP__(coalesce4)
		}
		coalesce5 := getGuestName()
		if __IS_SOME__(coalesce5) {
			return __UNWRAP__(coalesce5)
		}
		return "GUEST"
	}()

	timeout := IntOptionNone()
	finalTimeout := func() __INFER__ {
		coalesce6 := timeout
		if coalesce6.IsSome() {
			return coalesce6.Unwrap()
		}
		coalesce7 := parseEnv("TIMEOUT")
		if coalesce7.IsSome() {
			return coalesce7.Unwrap()
		}
		return getDefaultTimeout()
	}()

	count := IntOptionNone()
	total := (func() __INFER__ {
		if count.IsSome() {
			return __UNWRAP__(count)
		}
		return 10
	}()) * 2

	println("Display:", display)
	println("Port:", finalPort)
	println("Host:", host)
	println("Username:", username)
	println("Timeout:", finalTimeout)
	println("Total:", total)
}
func getDefaultName() string {
	return "Unknown User"
}
func getEnv(key string) StringOption {
	if key == "HOST" {
		return StringOptionSome("example.com")
	}
	return StringOptionNone()
}
func getConfig() ConfigOption {
	return ConfigOptionNone()
}
func findUser(id int) StringOption {
	return StringOptionNone()
}
func (s StringOption) toUpper() StringOption {
	if s.IsNone() {
		return StringOptionNone()
	}

	return s
}
func getGuestName() StringOption {
	return StringOptionNone()
}
func parseEnv(key string) IntOption {
	return IntOptionNone()
}
func getDefaultTimeout() int {
	return 30
}
