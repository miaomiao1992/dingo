package main

import (
	"os"
)

type User struct {
	ID   int
	Name string
}

func fetchUser(id int) (*User, error) {
	tmp1, err1 := os.ReadFile("user.json")

	if err1 != nil {
		return nil, err1
	}
	// dingo:e:1
	var data = tmp1
	return &User{ID: id, Name: string(data)}, nil
}
func getNames() ([]string, error) {
	tmp1, err1 := os.ReadFile("names.txt")

	if err1 != nil {
		return nil, err1
	}
	// dingo:e:1
	var data = tmp1
	return []string{string(data)}, nil
}
