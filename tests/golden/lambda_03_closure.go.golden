package main

func makeAdder(x int) func(int) int {

	return func(y int) int { x + y }
}
func makeCounter() func() int {
	count := 0

	return func() int {
		count = count + 1
		return count
	}
}
func main() {
	add5 := makeAdder(5)
	println("5 + 3 =", add5(3))

	counter := makeCounter()
	println("Count:", counter())
	println("Count:", counter())
}
