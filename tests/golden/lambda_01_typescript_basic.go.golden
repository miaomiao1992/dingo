package main

import (
	"fmt"
)

type User struct {
	Name string
	Age  int
}

func main() {

	double := func(x) { return x * 2 }
	result1 := double(5)

	triple := func(x) { return x * 3 }
	result2 := triple(5)

	add := func(x, y) { return x + y }
	result3 := add(10, 20)

	multiply := func(x int, y int) { return x * y }
	result4 := multiply(4, 7)

	numbers := []int{1, 2, 3, 4, 5}
	var doubled []int
	for _, n := range numbers {
		transform := func(x) { return x * 2 }
		doubled = append(doubled, transform(n))
	}

	apply := func(f func(int) int, val int) int {
		return f(val)
	}
	result5 := apply(func(x) { return x + 10 }, 5)

	fmt.Println(result1, result2, result3, result4, doubled, result5)
}
