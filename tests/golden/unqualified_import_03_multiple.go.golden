package main

import (
	"fmt"
	"os"
	"strconv"
)

import (
	"fmt"
	"os"
	"strconv"
)

func processInput(path string, numStr string) {

	data, err := os.ReadFile(path)
	if err != nil {
		panic(err)
	}

	num, err := strconv.Atoi(numStr)
	if err != nil {
		panic(err)
	}

	fmt.Printf("Read %d bytes, number is %d\n", len(data), num)
}
func main() {
	processInput("data.txt", "42")
}
