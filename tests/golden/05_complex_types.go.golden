package main

type User struct {
	ID   int
	Name string
}

func fetchUser(id int) (*User, error) {
	data, err := ReadFile("user.json")
	// dingo:s:1
	if err != nil {
		return nil, err
	}
	// dingo:e:1
	return &User{ID: id, Name: string(data)}, nil
}
func getNames() ([]string, error) {
	data, err := ReadFile("names.txt")
	// dingo:s:1
	if err != nil {
		return nil, err
	}
	// dingo:e:1
	return []string{string(data)}, nil
}
